var url;
$(function() {
	
	$('#cellMapSlider').bind({
					mouseenter : function(e){
						e.stopPropagation();
						var $cellMapSliderBG = $('#cellMapSliderBG');
						$(this).draggable({
							axis: "y",
							containment: "#slideArea",
							scroll:false, 
							grid:[0,18],
							drag: function() {
								var position = $(this).position();
								$cellMapSliderBG.css('height' , position.top+20); 
							},
							stop: function() {
								//var zoomIndex = parseInt($(this).position().top / 18);
								//if(zoomIndex === 4){
								//	CellMap.displayStarView(true);
								//}else{
								//	CellMap.displayStarView(false);
								//	CellMap.cellMapTargetScaleIndex = zoomIndex;
								//	CellMap.changeCellMapScale(zoomIndex);
								//}
							}
						});
					}
				});
	
	$( "#btnSelectFilter" ).draggable({
	axis: "x" 
	,containment: '#btnSelectFilterWrap'
	,stop: function( event, ui ) {
		if (ui.position.left >= 23) {
			//add stop event; $.util_pageMover.moveToTimeline(oFilter.o.timelineFilter);
		} else {
			$(ui.helper).css({
				left: 0
			});
		}
	}
});
	
	$("body").mCustomScrollbar({
	    scrollInertia: 150
	    ,theme: 'light'
	    ,advanced:{
	        updateOnContentResize: true
	    }
		,callbacks: {
			whileScrolling: function() {
				var top;
				if (mcs) {
					top = -mcs.top;
					if ($('#rail_0').length > 0) $('#rail_0').mCustomScrollbar("scrollTo", top);
					if ($('#rail_1').length > 0) $('#rail_1').mCustomScrollbar("scrollTo", top);
					if ($('#rail_2').length > 0) $('#rail_2').mCustomScrollbar("scrollTo", top);
				}
			}
		}
	});
	$("#wrapCellMap #div_srcoll_area").mCustomScrollbar({
	    scrollInertia: 150
	    ,theme: 'light'
	    ,advanced:{
	        updateOnContentResize: true
	    }
	});
	
});
